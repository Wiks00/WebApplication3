@model IEnumerable<WebApplication3.Models.ITEMORDER>

@{
    ViewBag.Title = "Index";
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Index</title>
</head>
<body>
    
    <table class="table">
        <tr>
            <th>
                Нзавание товара
            </th>
            <th>
                Цена
            </th>
            <th></th>
        </tr>
    
    @foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.ITEMINBASCKET.ITEMS.ITEMNAME)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.ITEMINBASCKET.ITEMS.PRISELIST.PRISE)
            </td>
            <td>
                @Html.ActionLink("Удалить", "Delete", new { id=item.ITEMORDERID })
            </td>
        </tr>
    }
    
    </table>
    <center>
        <div>
            @Html.ActionLink("Оформить", "Ok")
        </div>
    </center>
</body>
</html>
